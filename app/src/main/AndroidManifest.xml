<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mycam">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".bottom.ui.activity.DetailKamera" android:theme="@style/Theme.AppCompat.Light.DarkActionBar"></activity>
        <activity android:name=".bottom.ui.activity.PembelianActivity" android:theme="@style/Theme.AppCompat.Light.DarkActionBar"/>
        <activity android:name=".bottom.ui.activity.CategoryCamera" android:theme="@style/Theme.AppCompat.Light.DarkActionBar"/>
        <activity android:name=".bottom.ui.activity.DetailPemesanan" android:theme="@style/Theme.AppCompat.Light.DarkActionBar"/>
        <activity android:name=".bottom.BottomNavigation" android:theme="@style/Theme.AppCompat.Light.DarkActionBar"/>
            <activity android:name=".bottom.ui.activity.SplashActivity" android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>